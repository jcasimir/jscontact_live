<ul>
  <% @person.phone_numbers.each do |phone_number| %>
    <li id="phone_number_<%= phone_number.id %>">
      <%= phone_number.number %>
      <%= link_to "edit", edit_phone_number_path(phone_number), :id => "edit_phone_number_#{phone_number.id}" %>
      <%= link_to "delete",  phone_number_path(phone_number),
                  :method => :delete,
                  :id => "delete_phone_number_#{phone_number.id}"%>
    </li>
  <% end %>
</ul>